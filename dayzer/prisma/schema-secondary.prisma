generator client {
  provider = "prisma-client-js"
  output   = "./generated/secondary"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_SECONDARY")
}

model caiso_bess_fleet_data {
  utc_datetime_ib   DateTime @db.Timestamp(6)
  local_datetime_ib DateTime @db.Timestamp(6)
  entity            String   @db.VarChar(50)
  attribute         String   @db.VarChar(50)
  value             Decimal? @db.Decimal

  @@id([utc_datetime_ib, local_datetime_ib, entity, attribute])
}

model eox_forwards {
  curve_date     DateTime @db.Timestamp(6)
  iso            String   @db.VarChar(50)
  market         String   @db.VarChar(50)
  peak_hour      String   @db.VarChar(50)
  contract_begin DateTime @db.Timestamp(6)
  mid            Decimal? @db.Decimal
  bid            Decimal? @db.Decimal
  ask            Decimal? @db.Decimal
  atc            Decimal? @db.Decimal
  hr             Decimal? @db.Decimal

  @@id([curve_date, iso, market, peak_hour, contract_begin])
}

model eox_tags {
  iso       String @db.VarChar(50)
